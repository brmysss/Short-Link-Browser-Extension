{
  "manifest_version": 3,
  "name": "苏苏短链接生成器",
  "version": "1.0",
  "description": "将当前网页URL转换为短链接",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon16.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://s.brmys.cn/*",
    "https://api.dub.co/*",
    "https://dub.co/*",
    "https://dub.sh/*",
    "https://wz.my/*"
  ]
} 